{
  "name": "kramdown-ial-injection",
  "displayName": "Kramdown IAL",
  "description": "Adds syntax highlighting scopes in your markdown for Kramdown IAL (inline attribute lists) (e.g. {: .class #id key=\"value\"}) inside Markdown.",
  "version": "0.1.16",
  "publisher": "steveng57",
  "license": "MIT",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/steveng57/kramdown-ial-vscode.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "scripts": {
    "package": "npx @vscode/vsce package --no-dependencies"
  },
  "contributes": {
    "grammars": [
      {
        "scopeName": "text.kramdown-ial",
        "path": "./syntaxes/kramdown-ial.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ],
    "themes": [
      {
        "label": "Kramdown Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/Kramdown Dark-color-theme.json"
      },
      {
        "label": "Kramdown Light",
        "uiTheme": "vs",
        "path": "./themes/Kramdown Light-color-theme.json"
      }
    ]
  },
  "keywords": [
    "kramdown",
    "markdown",
    "jekyll",
    "ial",
    "inline attribute list",
    "syntax highlighting",
    "textmate",
    "theme",
    "dark theme",
    "light theme",
    "vscode theme"
  ]
}